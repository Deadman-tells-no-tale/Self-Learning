
def FindIntersection(strArr):
  check=0
  commanalities=""
  for counter in range (0,len(strArr)):
    if strArr[0][check] == strArr[1][counter]:
      commanalities += strArr[0][check] + ","
      check +=1

    else:
      check +=1
  commanalities.strip(",")
  # code goes here
  return commanalities

# keep this function call here 
print(FindIntersection(["1, 3, 4, 7, 13", "1, 2, 4, 13, 15"]))