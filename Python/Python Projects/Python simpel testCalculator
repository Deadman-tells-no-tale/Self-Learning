

num1 = float(input("Enter first integer: "))
operator = input("Enter Operator: ")
num2 = float(input("Enter first integer: "))

if operator == "+":
    print(num1 + num2)
elif operator == "-":
    print(num1 - num2)
elif operator == "*":
    print(num1 * num2)
elif operator == "/":
    print(num1 / num2)
else:
    print("invalid operator/number inputed")
    


